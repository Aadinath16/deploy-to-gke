{
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": "rc"
    },
    {
      "name": "feature",
      "prerelease": "alpha"
    }
  ],
    "npmPublish": false,
    "plugins": [
      [
        "@semantic-release/exec",
        {
          "successCmd": "echo v${nextRelease.version} > VERSION"
        }
      ]
    ]
  }
