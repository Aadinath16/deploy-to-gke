{
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": "rc"
    },
    {
      "name": "feature",
      "prerelease": "alpha"
    }
  ],
  "repositoryUrl": "https://github.com/Aadinath16/deploy-to-gke",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/npm",
    "@semantic-release/github"
  ],
  "tagFormat": "v${version}"
}



